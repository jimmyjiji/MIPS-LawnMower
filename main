.include "hw3"
.globl main
.text
main:

	li $v0, 13
	la $a0, filename
	li $a1, 0
	li $a2, 0
	
	syscall 
	
	move $a0, $v0        # load file descriptor
	li $v0, 14           #read from file
	la $a1, textSpace        # allocate space for the bytes loaded
	li $a2, 1000         # number of bytes to be read
	
	syscall  
	
	la $a0, textSpace        # address of string to be printed
	li $v0, 4            # print string
	
	syscall 


.data 
filename: .asciiz "C:\\Users\Jimmy\Desktop\hw3\hw3\checker.map" 
textSpace: .space 1050